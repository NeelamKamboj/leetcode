class Solution {
public:
    int countPoints(string rings) {
            vector<vector<int>> v(10,vector<int>(3,0));
        for(int i=0;i<rings.size();i=i+2){
            int no = rings[i+1]-'0';
            if(rings[i]=='B'){
                v[no][0]=1;
            }else if(rings[i]=='G'){
                v[no][1]=1;
            }else{
                v[no][2]=1;
            }
        }
        int cnt=0;
        for(int i=0;i<10;i++){
            if(v[i][0]==1 && v[i][1]==1 && v[i][2]==1){
                cnt++;
            } 
        }
        return cnt;
    }
};